{
  "name": "ganzyukv/wnet-test",
  "description": "Test on position PHP Developer in Wnet company",
  "keywords": ["wnet", "test"],
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Vladislav Ganzyuk",
      "email": "ganzyukv@gmail.com",
      "role": "Maintainer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "robmorgan/phinx": "^0.11.4",
    "symfony/dotenv": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "^1.9",
    "phpunit/phpunit": "^8.0",
    "symfony/var-dumper": "4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
